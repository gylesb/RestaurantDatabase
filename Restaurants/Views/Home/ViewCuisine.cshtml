<h1>Cuisine List</h1>

<table>
  <tr>
    <th>No.</th>
    <th>Cuisine</th>

  @if (@Model.Count == 0)
  {
    <h3>No cuisines have been added yet!</h3>
  }
  @foreach (var cuisine in Model)
  {
    <tr>
    <th>@cuisine.GetId()</th>
    <th><a href = '/@cuisine.GetName()/@cuisine.GetId()/restaurantlist'>@cuisine.GetName()</a> | <a href="/@cuisine.GetName()/@cuisine.GetId()/cuisine/delete">Delete</a></th>
    </tr>
  }
</table>
<br>
<a href="/cuisine/add">Add Cuisine</a><br>
<a href="/">Homepage</a>
